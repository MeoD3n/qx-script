[Mitm]
hostname =  www.patreon.com, shopee.vn, *-buy.itunes.apple.com, api.unlimapps.com, www.google.*, encrypted.google.*, raw.githubusercontent.com, gist.githubusercontent.com, spys.me,

[Script]
http-request (https:\/\/(.+\-|)buy\.itunes\.apple\.com\/WebObjects\/MZBuy.woa\/wa\/buyProduct)|(https:\/\/api\.unlimapps\.com\/.+\/apple_apps\/.+\/versions\?=) script-path = https://raw.githubusercontent.com/phd91105/Scripts/master/QuanX/scripting/old_ipa.js, requires-body = true
http-request https://shopee.vn.+me.+setting.+classic script-path = https://raw.githubusercontent.com/langkhach270389/Scripting/master/shopee_cookie.js, script-update-interval = 0
http-response (https:\/\/www\.google\..*)|(https:\/\/encrypted\.google\..*) script-path = https://raw.githubusercontent.com/phd91105/Scripts/master/QuanX/scripting/Endless_Google.user.js, requires-body = true
http-response ^https?:\/\/(raw|gist)\.githubusercontent.com/.*\.js$ script-path = https://raw.githubusercontent.com/Peng-YM/ScriptConverter/master/js-converter.js, requires-body = true
http-response https:\/\/www\.patreon\.com\/api\/oauth2\/v2\/identity? script-path = https://raw.githubusercontent.com/langkhach270389/Scripting/master/altstore.js, requires-body = true
http-response ^http:\/\/spys\.me\/proxy\.txt$ script-path=https://raw.githubusercontent.com/phd91105/Scripts/master/QuanX/scripting/proxy_test.js, requires-body=true
network-changed script-path=https://raw.githubusercontent.com/langkhach270389/Scripting/master/Surge/nextdns_linkedip.js,tag=nextdns_linkedip
